<%= form_for :company, url: companies_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :website %><br>
    <%= f.text_area :website %>
  </p>
   
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', companies_path %>